<fieldset>
  <legend><%= 'Topics' %></legend>
  <table class="table table-striped table-bordered">  
    <tr>
      <th><%= 'Email' %></th>
      <th><%= 'Topic' %></th>
      <th><%= 'Delete' %></th>
    </tr> 
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.email %></td>
        <td><%= contact.topic %></td>
        <td><%= link_to 'Delete', admin_contact_u_path(contact.id),:method => :delete, class: 'details'   %> </td>  
       </tr>
    <% end %>
  </table>
</fieldset>